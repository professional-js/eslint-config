{
	"extends": "@tsconfig/node12/tsconfig.json",
	"compilerOptions": {
		"esModuleInterop": true,
		"moduleResolution": "node",
		"strict": true,
		"outDir": "./.transpiled",
		"noUncheckedIndexedAccess": true,
		"noPropertyAccessFromIndexSignature": true,
		"typeRoots": [
			"./node_modules/@types/",
			"./typings",
		],
	},
	"include": [ "./" ],
}
